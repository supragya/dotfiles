colorscheme slate
syntax enable
set tabstop=4
set softtabstop=4
set expandtab
set number
set showcmd
set cursorline
filetype indent on
set showmatch
set incsearch
set hlsearch
set foldmethod=indent   
set foldnestmax=10
set nofoldenable
set foldlevel=2
set runtimepath^=~/.vim/bundle/ag
set foldmethod=syntax
